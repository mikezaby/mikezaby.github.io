{
  "name": "mikezaby.github.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && touch ./out/.nojekyll && echo \"mikezaby.com\" >> out/CNAME",
